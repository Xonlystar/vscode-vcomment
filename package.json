{
    "name": "vcomment",
    "publisher": "onlystar",
    "displayName": "vcomment",
    "description": "这是一个非常好用的函数注释插件, 你可以在这里很轻松的实现函数的注释",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
      "onLanguage:javascript",
      "onLanguage:vue",
      "onCommand:extension.comment"
    ],
    "main": "./src/extension",
    "contributes": {
      "commands": [{
        "command": "extension.comment",
        "title": "comment"
      }],
      "keybindings": [
        {
          "command": "extension.comment",
          "key": "alt+shift+d",
          "when": "editorTextFocus"
        }
      ]
    },
    "scripts": {
      "postinstall": "node ./node_modules/vscode/bin/install",
      "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
      "typescript": "^3.1.4",
      "vscode": "^1.1.25",
      "eslint": "^4.11.0",
      "@types/node": "^8.10.25",
      "@types/mocha": "^2.2.42"
    }
}